{
    "name": "bvkdev/categorizable",
    "description": "Implement the ability to categorize for custom models",
    "license": "MIT",
    "authors": [
        {
            "name": "Behrooz",
            "email": "behrooz.valikhani@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "kalnoy/nestedset": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "BvkDev\\Categorizable\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BvkDev\\Categorizable\\CategorizableServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
